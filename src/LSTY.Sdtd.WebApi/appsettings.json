{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "JwtConfig": {
    // 谁颁发的
    "Issuer": "lsty",
    // 颁发给谁
    "Audience": "lsty.sdtd.webapi",
    // 令牌密码
    "SigningKey": "24a30a82-add9-8ca0-8617-d46ef2a66237",
    // 过期时间(分钟)
    "AccessExpiration": 10,
    // 是否验证过期时间
    "ValidateLifetime": true
  },
  "AllowedHosts": "*"
}
